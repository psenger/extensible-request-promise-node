{
  "name": "extensible-request-promise-node",
  "version": "1.0.0",
  "description": "An Extensible Request Promisified for Node JS",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "typesVersions": {
    ">=3.1": {
      "*": [
        "ts3.1/*"
      ]
    }
  },
  "scripts": {
    "coverage": "jest --coverage --verbose",
    "test": "jest --ci",
    "build": "rollup -c",
    "deploy": "gatsby build --prefix-paths && gh-pages -d doc",
    "dev": "rollup -c -w"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/psenger/extensible-request-promise-node.git"
  },
  "keywords": [
    "request",
    "promise",
    "node"
  ],
  "author": "Philip A Senger <philip.a.senger@cngrgroup.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/psenger/extensible-request-promise-node/issues"
  },
  "homepage": "https://psenger.github.io/extensible-request-promise-node",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^11.0.1",
    "@rollup/plugin-node-resolve": "^7.0.0",
    "gatsby": "^2.31.1",
    "gh-pages": "^3.1.0",
    "jest": "^26.6.3",
    "jest-html-reporters": "^2.1.2",
    "jest-junit": "^12.0.0",
    "nock": "^13.0.6",
    "rollup": "^2.38.0",
    "rollup-plugin-copy": "^3.3.0"
  },
  "jest": {
    "reporters": [
      "default",
      [
        "jest-junit",
        {
          "suiteName": "jest tests",
          "outputDirectory": "junit",
          "outputName": "junit.xml",
          "uniqueOutputName": "false",
          "classNameTemplate": "{classname}-{title}",
          "titleTemplate": "{classname}-{title}",
          "ancestorSeparator": " â€º ",
          "usePathForSuiteName": "true"
        }
      ],
      [
        "jest-html-reporters",
        {
          "publicPath": "./report",
          "filename": "index.html",
          "expand": true
        }
      ]
    ]
  }
}
